        <Setter Property="Foreground" Value="#808080" />
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="FontFamily" Value="Consolas" />
        <Setter Property="Effect">
